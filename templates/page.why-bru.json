    "16114845-b71d-46c7-b179-fefe506ec5b3": {
      "type": "rich-text",
      "blocks": {
        "template--16114935464097__16114845-b71d-46c7-b179-fefe506ec5b3-heading-1": {
          "type": "heading",
          "settings": {
            "text": "Warum wir tun, was wir tun.",
